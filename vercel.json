{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "installCommand": "npm install",
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "functions": {
        "app/api/**/*.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "ignoreCommand": "if [ \"$VERCEL_ENV\" = \"production\" ]; then npx vercel-ignore; else exit 0; fi"
}